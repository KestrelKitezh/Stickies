<Window x:Class="Codename_Stickies.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Codename_Stickies"
        mc:Ignorable="d"
        Title="MainWindow" Height="60" Width="60" BorderThickness="0" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True" WindowState="Normal" WindowStyle="None"
        AllowsTransparency="True">
    <Window.Background>
        <SolidColorBrush Opacity="0" Color="Black"></SolidColorBrush>
    </Window.Background>
    <Grid>
        <Image HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="None" x:Name="ImgFrame" />
    </Grid>
</Window>
